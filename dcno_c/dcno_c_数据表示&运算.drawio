        <mxCell id="Larsk8cx2z01rOeCRQHz-7" value="for unsigned num" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="1">
